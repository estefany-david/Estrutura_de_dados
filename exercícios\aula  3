//3)
//tabela da feira.
#include<stdio.h>
int main(void){
  printf("produto |preço unitario |quantidade |preço total\n ");
  printf("banana  |R$%10.2f\t|%10d |R$%10.2f     \n",2.50,2,5.00);
  printf("uva     |R$%10.2f\t|%10d |R$%10.2f     \n",6.50,6,39.00);
  printf("pessego |R$%10.2f\t|%10d |R$%10.2f     \n",10.22,10,102.20);
  printf("\t|\t\t |subtotal  |R$%10.2f    \n ",146.20);
  printf("\t|imposto\t\t     |R$%10.2f    \n ",7.31);
  printf("\t|\t\t |total     |R$%10.2f  \n ",153.51);
}
